{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id", "__v"] }],
    "no-console": "off",
    "constructor-return": "off",
    "no-param-reassign": "off",
    "consistent-return": "off",
    "no-unused-vars": ["error", { "args": "none" }],
    "max-len": ["error", { "code": 160 }],
    "no-useless-escape": "off"
  }
}
